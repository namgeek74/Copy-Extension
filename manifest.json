{
    "name": "Copy Extension",
    "version": "1.0",
    "description": "Double click to copy code!",
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "tabs",
        "contextMenus"
    ],
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://stackoverflow.com/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}